{
  "name": "vue_typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "ant-design-vue": "^1.4.6",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "compression-webpack-plugin": "^3.0.0",
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.1.0",
    "typescript": "^3.7.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard",
      "@vue/typescript"
    ],
    "rules": {
      "quotes": [
        1,
        "single"
      ],
      "semi": [
        "error",
        "never"
      ],
      "eqeqeq": 0,
      "no-unused-vars": 0,
      "space-before-function-paren": 0,
      "prefer-const": 0,
      "no-undef": 0,
      "camelcase": 0,
      "indent": [
        0,
        4
      ],
      "no-tabs": 0,
      "eol-last": 0,
      "space-before-blocks": 0,
      "comma-dangle": 0,
      "no-multiple-empty-lines": [
        1,
        {
          "max": 2
        }
      ],
      "no-trailing-spaces": 0,
      "spaced-comment": 0,
      "key-spacing": [
        0,
        {
          "beforeColon": false,
          "afterColon": true
        }
      ],
      "space-infix-ops": 0,
      "comma-spacing": 0,
      "require-v-for-key": 0,
      "no-side-effects-in-computed-properties": 0,
      "object-curly-even-spacing": 0,
      "no-mixed-spaces-and-tabs": 0,
      "eslint-plugin-vue": 0,
      "no-multi-spaces": 0,
      "func-call-spacing": 0
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
